{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": [
                "*"
            ]
        }
    },
    "include": [
        "src",
        "vite.config.ts"
    ]
}